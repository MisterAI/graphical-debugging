<?xml version="1.0" encoding="utf-8"?>
<GraphicalDebugging>

  <!--
  struct tagPOINT {
    long x;
    long y;
  };
  -->
  <Point Id="tagPOINT">
    <Coordinates>
      <X>x</X>
      <Y>y</Y>
    </Coordinates>
  </Point>

  <!--
  namespace boost { namespace qvm {
  template <class T,int Dim>
  struct vec
  {
      T a[Dim];
  };
  }}
  -->
  <Point Id="boost::qvm::vec">
    <Coordinates>
      <X>a[0]</X>
      <Y>a[1]</Y>
    </Coordinates>
  </Point>

  <!--
  struct MyPoint
  {
      double x, y;
  };
  -->
  <Point Id="MyPoint">
    <Coordinates>
      <X>x</X>
      <Y>y</Y>
    </Coordinates>
  </Point>

  <!--
  struct MyLinestring : std::vector<MyPoint>
  { };
  -->
  <Linestring Id="MyLinestring">
    <Points>
      <Container>
        <Name>(std::vector&lt;MyPoint,std::allocator&lt;MyPoint&gt; &gt;&amp;)(*this)</Name>
      </Container>
    </Points>
  </Linestring>

  <!--
  struct MyLinestring2
  {
      std::vector<MyPoint> points;
  };
  -->
  <Linestring Id="MyLinestring2">
    <Points>
      <Container>
        <Name>points</Name>
      </Container>
    </Points>
  </Linestring>

  <!--
  struct MyLinestring3
  {
      MyPoint * begin;
      MyPoint * end;
  };
  -->
  <Linestring Id="MyLinestring3">
    <Points>
      <Array>
        <Pointer>begin</Pointer>
        <Size>end - begin</Size>
      </Array>
    </Points>
  </Linestring>

  <!--
  struct MyRing : std::vector<MyPoint>
  { };
  -->
  <Ring Id="MyRing">
    <Points>
      <Container>
        <Name>(std::vector&lt;MyPoint,std::allocator&lt;MyPoint&gt; &gt;&amp;)(*this)</Name>
      </Container>
    </Points>
  </Ring>

  <!--
  struct MyPolygon : std::vector<MyRing>
  { };
  -->
  <Polygon Id="MyPolygon">
    <ExteriorRing>
      <Name>(*_Mypair._Myval2._Myfirst)</Name>
    </ExteriorRing>
    <InteriorRings>
      <Container>
        <Name>(std::vector&lt;MyRing,std::allocator&lt;MyRing&gt; &gt;&amp;)(*this)</Name>
        <Offset>1</Offset>
      </Container>
    </InteriorRings>
  </Polygon>

  <!--
  struct MyMultiPoint : std::vector<MyPoint>
  { };
  -->
  <MultiPoint Id="MyMultiPoint">
    <Points>
      <Container>
        <Name>(std::vector&lt;MyPoint,std::allocator&lt;MyPoint&gt; &gt;&amp;)(*this)</Name>
      </Container>
    </Points>
  </MultiPoint>

  <!--
  struct MyMultiLinestring : std::vector<MyLinestring>
  { };
  -->
  <MultiLinestring Id="MyMultiLinestring">
    <Linestrings>
      <Container>
        <Name>(std::vector&lt;MyLinestring,std::allocator&lt;MyLinestring&gt; &gt;&amp;)(*this)</Name>
      </Container>
    </Linestrings>
  </MultiLinestring>

  <!--
  struct MyMultiPolygon : std::vector<MyPolygon>
  { };
  -->
  <MultiPolygon Id="MyMultiPolygon">
    <Polygons>
      <Container>
        <Name>(std::vector&lt;MyPolygon,std::allocator&lt;MyPolygon&gt; &gt;&amp;)(*this)</Name>
      </Container>
    </Polygons>
  </MultiPolygon>

</GraphicalDebugging>